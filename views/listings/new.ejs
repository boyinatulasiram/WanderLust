<% layout("/layouts/boilerplate") %>
<body>
    <h1>Create a new Listing</h1>
    <form action="/listings" method="POST">
        <input type="text" name="listing[title]" placeholder="enter title">
        <br><br>
        <textarea name="listing[description]" placeholder="enter description" ></textarea>
        <br><br>
        <input type="text" name="listing[image][url]" placeholder="enter image url">
        <br><br>
        <input type="number" name="listing[price]" placeholder="enter price">
        <br>
        <input
         type="text" name="listing[country]" placeholder="enter country">
        <br><br>
        <input type="text" name="listing[location]" placeholder="enter location">
        <button>Add</button>
    </form>
</body>